				<LayerMain href="Textures/Pressed.(WidgetLayerSimpleTexture).xdb#xpointer(/WidgetLayerTiledTexture)" />
			</StatePushed>
			<PushedOffset x="0" y="0" />
			<ReactionOnUp>true</ReactionOnUp>
			<Reaction>OnBuffClick</Reaction>
			<ReactionRightClick>OnRemoveBuff</ReactionRightClick>
		</Item>
	</Variants>
	<Visible>false</Visible>
</WidgetButton>
