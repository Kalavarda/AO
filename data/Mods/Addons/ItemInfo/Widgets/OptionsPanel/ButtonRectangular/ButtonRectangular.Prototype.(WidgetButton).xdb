<?xml version="1.0" encoding="UTF-8" ?>
<WidgetButton>
	<Header>
		<isPrototype>true</isPrototype>
	</Header>
	<Placement>
		<X>
			<Size>96</Size>
		</X>
		<Y>
			<Size>32</Size>
		</Y>
	</Placement>
	<IgnoreDblClick>true</IgnoreDblClick>
	<TextTag>button_label</TextTag>
	<Variants>
		<Item>
			<TextFileRef href="DefaultValue.txt" />
			<LayerHighlight href="OptionBtnHighlight.(WidgetLayerTiledTexture).xdb#xpointer(/WidgetLayerTiledTexture)" />
			<StateNormal>
				<LayerMain href="OptionBtnNormal.(WidgetLayerTiledTexture).xdb#xpointer(/WidgetLayerSimpleTexture)" />
				<FormatFileRef href="ButtonFormatNormal.txt" />
			</StateNormal>
			<!--<StateHighlighted>
				<FormatFileRef href="ButtonFormatHighlighted.txt" />
			</StateHighlighted>-->
			<StateDisabled>
				<LayerMain href="ButtonDisabled.(WidgetLayerSimpleTexture).xdb#xpointer(/WidgetLayerSimpleTexture)" />
			</StateDisabled>
			<Reaction>button_click</Reaction>
			<ReactionOnUp>true</ReactionOnUp>
		</Item>
	</Variants>
	<TextStyle>
		<wrapText>false</wrapText>
		<Align>ALIGNY_MIDDLE</Align>
	</TextStyle>
</WidgetButton>